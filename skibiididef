--Chapter 3 | Functions & Locals

local Players = game:GetService("Players")
local Player = Players.LocalPlayer
local Mouse = Player:GetMouse()
local Lighting = game.Lighting
local Character = Player.Character
local Humanoid = Character.Humanoid
local ReplicatedStorage = game.ReplicatedStorage
local Interaction = ReplicatedStorage.Interaction
local Workspace = game.Workspace
local Jihad = task
local HRP = Character.HumanoidRootPart
local oldPos = HRP.CFrame
Day = false
Night = false
Fog = false 
Slot = 0


local LuaWareEngineVariables = {
    ClientDrag = Interaction.ClientIsDragging,
    BlueprintRemote = ReplicatedStorage.PlaceStructure.ClientPlacedBlueprint,
    PropertyExpanded = ReplicatedStorage.PropertyPurchasing.ClientExpandedProperty,
    PropertyPurchased = ReplicatedStorage.PropertyPurchasing.ClientPurchasedProperty,
    RequestLoad = ReplicatedStorage.LoadSaveRequests.RequestLoad,
    RemoteProxy = ReplicatedStorage.Interaction.RemoteProxy,
    GreenBox = Workspace["Region_Volcano"].VolcanoWin,
    InstallingPropertyPaths = Workspace.Properties,
	LoadBase = ReplicatedStorage.LoadSaveRequests.RequestLoad,
	PlayerModel = Workspace.PlayerModels:GetChildren(),
	SeatPart = Humanoid.SeatPart
}

local function LightingChanged()
    if Day then
        Lighting.TimeOfDay = "12:00:00"
        Lighting.SunPos.Value = 1
        Lighting.Ambient = Color3.new(1, 1, 1)
        Lighting.ColorShift_Bottom = Color3.new(1, 1, 1)
        Lighting.ColorShift_Top = Color3.new(1, 1, 1)
    elseif Night then
        Lighting.TimeOfDay = "00:00:00"
        Lighting.SunPos.Value = 0
    elseif Fog then
        Lighting.FogEnd = math.huge
    end
end


Lighting.Changed:Connect(LightingChanged)

local LuaWareEngineFunctions = {}

function LuaWareEngineFunctions:Speed()
	game.Players.LocalPlayer.Character.Humanoid:GetPropertyChangedSignal("WalkSpeed"):Connect(function()
	game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = getgenv().Speed
 end)

 game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = getgenv().Speed
 end

function LuaWareEngineFunctions:GetPlrs()
	local Plrs = {}
	for i,v in next, game.Players:GetPlayers() do
		if not table.find(Plrs,v.Name) then
			table.insert(Plrs,v.Name)
		end
	end
	return Plrs
end

function LuaWareEngineFunctions:Noclip(Value)
	local Character = game.Players.LocalPlayer.Character
	
		for i,v in pairs(Character:GetDescendants()) do
		if v.ClassName == "Part" or v.ClassName == "MeshPart" then
			if Value then
				v.CanCollide = false
			else
				v.CanCollide = true
			end
		end
	end
end
	
function LuaWareEngineFunctions:CarBringPlayer()
	--Method From Silent Ben8x <3
	LuaWareEngineFunctions:GetCurrentPos()
	repeat Jihad.wait() LuaWareEngineVariables.SeatPart.Parent:PivotTo(game.Players[User].Character.HumanoidRootPart.CFrame, (Player))
	LuaWareEngineVariables.SeatPart.Parent:SetPrimaryPartCFrame(SeatPart.CFrame * CFrame.Angles(math.rad(180),0,0)) --Snatched This From VehicleFlip :flush:
	until game.Players[User].Character.Humanoid.SeatPart
	game.Players.LocalPlayer.Character.Humanoid.SeatPart.Parent:PivotTo(LuaWareEngineFunctions:OldPos(),(HRP))
end

function LuaWareEngineFunctions:SellAllLogs()	
	for _, wood in ipairs(workspace.LogModels:GetChildren()) do
		if wood:FindFirstChild("Owner") and wood.Owner.Value == Player then
			HRP.CFrame = wood.WoodSection.CFrame
			Jihad.wait(0.5)
			
			for _ = 1, 20 do
				for _, woodSection in ipairs(wood:GetChildren()) do
					if woodSection.Name == "WoodSection" then
						spawn(function()
							for _ = 1, 10 do
								Interaction.ClientIsDragging:FireServer(wood)
								wood:PivotTo(CFrame.new(314.20575, -0.400000274, 85.7978668, 0.94260478, -7.16537585e-08, -0.333910495, 5.62629374e-08, 1, -5.57635822e-08, 0.333910495, 3.37762351e-08, 0.94260478))
								woodSection.CFrame = CFrame.new(314.20575, -0.400000274, 85.7978668, 0.94260478, -7.16537585e-08, -0.333910495, 5.62629374e-08, 1, -5.57635822e-08, 0.333910495, 3.37762351e-08, 0.94260478)
								Interaction.ClientIsDragging:FireServer(wood)
								Jihad.wait()
							end
							HRP.CFrame = CFrame.new(314.20575, -0.400000274, 85.7978668, 0.94260478, -7.16537585e-08, -0.333910495, 5.62629374e-08, 1, -5.57635822e-08, 0.333910495, 3.37762351e-08, 0.94260478)
						end)
						Jihad.wait()
					end
				end
				Jihad.wait(0.1)
			end
			Jihad.wait(0.3)
		end
	end
	
	Jihad.wait()
	HRP.CFrame = oldPos	
--Notify('LuaWare Softwares', 'Sell Log(s) Completed', 2)  
end

function VehicleSpeed(Value)

end	

function LuaWareEngineFunctions:VehicleFlip()
	local HSS = Humanoid.SeatPart
	HSS.Parent:SetPrimaryPartCFrame(HSS.Parent.PrimaryPart.CFrame*CFrame.Angles(math.rad(-180),0,0) + Vector3.new(0,5,0))
end



function LuaWareEngineFunctions:AxeDuper(Slot)
	repeat Jihad.wait() until ReplicatedStorage.LoadSaveRequests.ClientMayLoad:InvokeServer(Player)
	if DupeTimer > 1 then
		LuaWareEngineVariables.RequestLoad:InvokeServer(Slot, Player)
		Jihad.wait(DupeTimer)
		Character.HumanoidRootPart.CFrame = CFrame.new(187.845, -393.454, -516.77)
		repeat Jihad.wait() until Player.OwnsProperty.Value == true
        repeat Jihad.wait() until ReplicatedStorage.LoadSaveRequests.ClientMayLoad:InvokeServer(Player)
	end
	Character.HumanoidRootPart.CFrame = CFrame.new(187.845, -393.454, -516.77)
	Jihad.wait(2)
	LuaWareEngineVariables.RequestLoad:InvokeServer(Slot, Player)
	repeat Jihad.wait() until Player.OwnsProperty.Value == true
	repeat Jihad.wait() until ReplicatedStorage.LoadSaveRequests.ClientMayLoad:InvokeServer(Player)
end

function LuaWareEngineFunctions:Buy(ID)
	spawn(function()
		ReplicatedStorage.NPCDialog.PlayerChatted:InvokeServer({["ID"] = ID,["Character"] = "name",["Name"] = "name",["Dialog"] = 'Dialog'}, "ConfirmPurchase");
	end)
end

function LuaWareEngineFunctions:LoadTeleportOff()
	local HRP = Character.HumanoidRootPart
	local Clone = HRP:Clone()
	HRP:Destroy()
	Clone.Parent = Character
end

function LuaWareEngineFunctions:FreeProperty()

end

function LuaWareEngineFunctions:GoodFPS()
	for _,specialist in pairs(workspace:GetDescendants()) do
		if specialist.ClassName == "Part"
		or specialist.ClassName == "SpawnLocation"
		or specialist.ClassName == "WedgePart"
		or specialist.ClassName == "Terrain"
		or specialist.ClassName == "MeshPart" then
			specialist.Material = "Plastic"
		end
		end
end

function LuaWareEngineFunctions:VeryGoodFPS()
	for _,specialist in pairs(workspace:GetDescendants()) do
		if specialist.ClassName == "Part"
		or specialist.ClassName == "SpawnLocation"
		or specialist.ClassName == "WedgePart"
		or specialist.ClassName == "Terrain"
		or specialist.ClassName == "MeshPart" then
			specialist.Material = "Plastic"
		end
		end

		for _,specialist in pairs(workspace:GetDescendants()) do
			if specialist.ClassName == "Decal"
			or specialist.ClassName == "Texture" then
			v:Destroy()
			end
			end
end

function LuaWareEngineFunctions:AntiFling()
	--Credits to dude who made it
        _G.AntiFlingConfig = {
        disable_rotation = true;
        limit_velocity = true;
        limit_velocity_sensitivity = Value;
        limit_velocity_slow = 0;
        
        anti_ragdoll = true;
        
        anchor = false;
        smart_anchor = true; 
        anchor_dist = Value; --
        
        teleport = false;
        smart_teleport = true; 
        teleport_dist = 30;
    }
    loadstring(game:HttpGet("https://pastebin.com/raw/rhzYvadM",true))()
end

--Item Teleport RECODED

local ItemType, ItemOwner, IncludeBoxes, ToBase = "BasicHatchet", game.Players.LocalPlayer, false, false

local Plr, Char = game.Players.LocalPlayer, game.Players.LocalPlayer.Character

function LuaWareEngineFunctions:GetCurrentPos()
	return Char.HumanoidRootPart.CFrame
end

function LuaWareEngineFunctions:GetPing()
    local Time = tick()
    game.ReplicatedStorage.TestPing:InvokeServer()
    return ((tick() - Time) / 2) + .1
end

function LuaWareEngineFunctions:Teleport(Pos)
	Char.HumanoidRootPart.CFrame = Pos
end

function LuaWareEngineFunctions:GetProperty(Value)
    for i, v in next, Workspace.Properties:GetChildren() do
        if v:FindFirstChild"Owner" and v.Owner.Value == Value then
            return v
        end
    end
    return false
end

function LuaWareEngineFunctions:MoveItem(Model, Iteration, Pos)
	if not Model.PrimaryPart then
        Model.PrimaryPart = Model:FindFirstChildOfClass"Part"
    end

    if Model:FindFirstChild"ItemName" then
        game.ReplicatedStorage.PlaceStructure.ClientPlacedStructure:FireServer(nil, Pos, Plr, nil, Model, true)
        Jihad.wait(1.3)
        return
    end

    pcall(function()
        for i = 1, Iteration do
            Interaction.ClientIsDragging:FireServer(Model)
            Model:PivotTo(Pos)
            Interaction.ClientIsDragging:FireServer(Model)
            Jihad.wait(LuaWare:GetPing())
        end
    end)
end

function LuaWareEngineFunctions:MoveItem(Model, Iteration, Pos)
	if ToBase and not LuaWare:GetProperty(Owner) then
        return print"Player does not own a plot" -- change to a notifcation
    end

    local OldPos, ItemFound = ToBase and LuaWare:GetProperty(Owner).OriginSquare.CFrame + Vector3.new(0, 3, 0) or LuaWare:GetCurrentPos(), false

    for i, v in next, Workspace.PlayerModels:GetChildren() do
        if v:FindFirstChild"Owner" and v.Owner.Value == Owner then
            if v:FindFirstChild"Type" and v.Type.Value == Type or v:FindFirstChild"WoodClass" and v.WoodClass.Value == Type or v:FindFirstChild"ItemName" and v.ItemName.Value == Type or v:FindFirstChild"PurchasedBoxItemName" and v.PurchasedBoxItemName.Value == Type and IncludeBoxes then
                ItemFound = true
                if (Char.Head.CFrame.p - v:FindFirstChildOfClass"Part".CFrame.p).Magnitude >= 25 then
                    LuaWare:Teleport(CFrame.new(v:FindFirstChildOfClass"Part".CFrame.p))
                end
                LuaWare:MoveItem(v, 35, OldPos)
                Jihad.wait()
            end
        end
    end

    if not ItemFound then
        return print"No Items Found"
    end

    LuaWare:Teleport(OldPos)
end

function LuaWareEngineFunctions:RemoveSnowBoulders(Value)
    for i,v in next,game.Workspace.Region_Snow.PartSpawner:GetChildren() do
        if v.Name == "Boulder" then
            if Value then
                v.Transparency = 1
                v.CanCollide = false
                else
                v.Transparency = 0
                v.CanCollide = true
            end
        end
    end
end

function LuaWareEngineFunctions:AntiBring()
    for Index,Specialist in pairs(Workspace:GetDescendants()) do
        if Specialist.Name == "Seat" then
            Specialist:Destroy()
        end
    end
end

function LuaWareEngineFunctions:SolidWater(Value)
    for Index, Specialist in ipairs(game.Workspace.Water:GetChildren()) do
        if Specialist.Name == "Water" then
        if Value then
        Specialist.CanCollide = true
        else
        Specialist.CanCollide = false
        end
    end
end
end


function LuaWareEngineFunctions:HideWater(Value)
    for Index, Specialist in ipairs(game.Workspace.Water:GetChildren()) do
        if Specialist.Name == "Water" then
        if Value then
        Specialist.Transparency = 1
        else
        Specialist.Transparency = 0
        end
    end
end
end

function LuaWareEngineFunctions:BetterWater()
    for i,v in next, game.Workspace:GetDescendants() do
        if v:IsA("Part") and v.Name == "SeaSand" then
        v.Size = Vector3.new(2048, 60, 2048)
    end
    end

    for i,v in next, game.Workspace:GetDescendants() do
    if v:IsA("Part") and v.Name == "Water" then
        v.Size = Vector3.new(20480, 6, 20080)
        game.Workspace.Terrain:fillBlock(v.CFrame, v.Size, Enum.Material.Water)
        v:Destroy()
    end
    end
end

function LuaWareEngineFunctions:BridgeLowering(Value)

end

function LuaWareEngineFunctions:TeleportToBase()
for Index, Specialist in ipairs(LuaWareEngineVariables.InstallingPropertyPaths:GetChildren()) do
        if Specialist:IsA("Model") and Specialist:FindFirstChild("OriginSquare") and Specialist:FindFirstChild("Owner") and Specialist.Owner.Value == Player then
        Jihad.wait()
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = Specialist.OriginSquare.CFrame
        end
    end
end

function LuaWareEngineFunctions:OldPos()
    local originalPosition = Player.Character.HumanoidRootPart.Position
    return originalPosition
end

function LuaWareEngineFunctions:ReturnOldPos()
    Player.Character.HumanoidRootPart.CFrame = CFrame.new(originalPosition)
end

function LuaWareEngineFunctions:AntiAFK(Val)
local GC = getconnections or get_signal_cons
if GC then
    for i,v in pairs(GC(Players.LocalPlayer.Idled)) do
        if v["Disable"] then
            v["Disable"](v)
        elseif v["Disconnect"] then
            v["Disconnect"](v)
        end
    end
else
    Players.LocalPlayer.Idled:Connect(function()
        local VirtualUser = game:GetService("VirtualUser")
        VirtualUser:CaptureController()
        VirtualUser:ClickButton2(Vector2.new())
    end)
end
end

function LuaWareEngineFunctions:ServerHop()
    loadstring(game:HttpGet"https://raw.githubusercontent.com/LeoKholYt/roblox/main/lk_serverhop.lua")():Teleport(13822889)
end

function LuaWareEngineFunctions:RejoinServer()
	--Thanks silent for removing 2 useless lines for me <3
	game:GetService("TeleportService"):TeleportToPlaceInstance(game.PlaceId,game.JobId, game.Players.LocalPlayer)
end


function LuaWareEngineFunctions:DestroyGreenBox()
	--Thanks silent for the method <3
    for Index, Specialist in ipairs(game.Workspace["Region_Volcano"]:GetChildren()) do
        if Specialist.Name == "VolcanoWin" then
            LuaWareEngineFunctions:OldPos()
            Player.Character.HumanoidRootPart.CFrame = Specialist.CFrame
            LuaWareEngineVariables.ClientDrag:FireServer(Specialist)
            LuaWareEngineVariables.GreenBox.BodyPosition:Destroy()
            LuaWareEngineVariables.GreenBox.BodyAngularVelocity:Destroy()
            Jihad.wait(1)
        end
    end
    LuaWareEngineFunctions:ReturnOldPos()
end


--UI LIB :flush: <3 Silent Bin8fucks jk his good
local library = {}
library.flags = {}
library.currentTab = nil
local toggled = false
local mouse = game.Players.LocalPlayer:GetMouse()

local theme = {
	main = Color3.fromRGB(37, 37, 37),
	secondary = Color3.fromRGB(42, 42, 42),
	accent = Color3.fromRGB(255, 255, 255),
	accent2 = Color3.fromRGB(57, 57, 57)
}

function Tween(obj,size,delay)
	obj:TweenSize(size,"Out","Sine",delay,false)
end

function Tween2(obj, t, data)
	game:GetService("TweenService"):Create(obj, TweenInfo.new(t[1], Enum.EasingStyle[t[2]], Enum.EasingDirection[t[3]]), data):Play()
	return true
end

function Ripple(obj)
	spawn(function()
		if obj.ClipsDescendants ~= true then
			obj.ClipsDescendants = true
		end
		local Ripple = Instance.new("ImageLabel")
		Ripple.Name = "Ripple"
		Ripple.Parent = obj
		Ripple.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
		Ripple.BackgroundTransparency = 1.000
		Ripple.ZIndex = 8
		Ripple.Image = "rbxassetid://2708891598"
		Ripple.ImageTransparency = 0.800
		Ripple.ScaleType = Enum.ScaleType.Fit
		Ripple.ImageColor3 = Color3.fromRGB(0,0,0)
		Ripple.Position = UDim2.new((mouse.X - Ripple.AbsolutePosition.X) / obj.AbsoluteSize.X, 0, (mouse.Y - Ripple.AbsolutePosition.Y) / obj.AbsoluteSize.Y, 0)
		Tween2(Ripple, {.3, 'Linear', 'InOut'}, {Position = UDim2.new(-5.5, 0, -5.5, 0), Size = UDim2.new(12, 0, 12, 0)})
		wait(0.15)
		Tween2(Ripple, {.3, 'Linear', 'InOut'}, {ImageTransparency = 1})
		wait(.3)
		Ripple:Destroy()
	end)
end

local changeingTab = false
function SwitchTab(Tab)
	if changeingTab == true then print("Changing Tab") return end
	local Old = library.currentTab
	if Old == nil then
		library.currentTab = Tab[1]
		Tab[1].Visible = true
		return
	end
	if Tab[1].Visible == true then return end
	changeingTab = true
	Tween(Old.Parent,UDim2.new(0, 440,0, 0),.15)
	Old.Visible = false
	wait(0.5)
	Tab[1].Visible = true
	Tween(Tab[1].Parent,UDim2.new(0, 440,0, 318),.15)
	library.currentTab = Tab[1]
	wait(.1)
	changeingTab = false
end

function drag(frame, hold) -- Skidded from Kiriot or Wally ~V3rmillion~
	if not hold then
		hold = frame
	end
	local dragging
	local dragInput
	local dragStart
	local startPos

	local function update(input)
		local delta = input.Position - dragStart
		frame.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
	end

	hold.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 then
			dragging = true
			dragStart = input.Position
			startPos = frame.Position

			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)

	frame.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement then
			dragInput = input
		end
	end)

	game:GetService("UserInputService").InputChanged:Connect(function(input)
		if input == dragInput and dragging then
			update(input)
		end
	end)
end

function library:Create(title)
	local Atomic = Instance.new("ScreenGui")
	local MainFrame = Instance.new("Frame")
	local MainFrameC = Instance.new("UICorner")
	local SideFrame = Instance.new("Frame")
	local SideFrameC = Instance.new("UICorner")
	local TabContainer = Instance.new("ScrollingFrame")
	local TabOpen = Instance.new("TextButton")
	local TabContainerL = Instance.new("UIListLayout")
	local TabContainerP = Instance.new("UIPadding")
	local TabHolder = Instance.new("Frame")
	local TabHolderC = Instance.new("UICorner")
	local TopFrame = Instance.new("Frame")
	local SideFrameC_2 = Instance.new("UICorner")
	local Title = Instance.new("TextLabel")
	local Glow = Instance.new("ImageLabel")

	function DestroyUI()
		Atomic:Destroy()
	end

	Atomic.Name = "Atomic"
	Atomic.Parent = game:GetService("RunService"):IsStudio() and game.Players.LocalPlayer:WaitForChild("PlayerGui") or game:WaitForChild("CoreGui")

	MainFrame.Name = "MainFrame"
	MainFrame.Parent = Atomic
	MainFrame.BackgroundColor3 = theme.main
	MainFrame.Position = UDim2.new(0.345118761, 0, 0.277912617, 0)
	MainFrame.Size = UDim2.new(0, 587, 0, 366)
	MainFrame.BorderSizePixel = 0

	--	

	function ToggleUI()
		toggled = not toggled
		print(toggled)
		if not MainFrame.ClipsDescendants then
			MainFrame.ClipsDescendants = true
		end
		if toggled then
			Tween(MainFrame,UDim2.new(0, 587,0, 0),0.15)
		else
			Tween(MainFrame,UDim2.new(0,587,0,366),0.15)
		end
	end

	MainFrameC.CornerRadius = UDim.new(0, 4)
	MainFrameC.Name = "MainFrameC"
	MainFrameC.Parent = MainFrame

	SideFrame.Name = "SideFrame"
	SideFrame.Parent = MainFrame
	SideFrame.BackgroundColor3 = theme.secondary
	SideFrame.Position = UDim2.new(0.0120000103, 0, 0.117999971, 0)
	SideFrame.Size = UDim2.new(0, 130, 0, 318)

	SideFrameC.CornerRadius = UDim.new(0, 4)
	SideFrameC.Name = "SideFrameC"
	SideFrameC.Parent = SideFrame
	
	Glow.Name = "Glow"
	Glow.Parent = MainFrame
	Glow.AnchorPoint = Vector2.new(0.5, 0.5)
	Glow.BackgroundTransparency = 1.000
	Glow.BorderSizePixel = 0
	Glow.Position = UDim2.new(0.5, 0, 0.5, 0)
	Glow.Size = UDim2.new(1, 47, 1, 47)
	Glow.ZIndex = 0
	Glow.Image = "rbxassetid://6014261993"
	Glow.ImageTransparency = 0.500
	Glow.ScaleType = Enum.ScaleType.Slice
	Glow.SliceCenter = Rect.new(49, 49, 450, 450)
	
	Jihad.spawn(function()
		while Glow do
			Glow.ImageColor3 = Color3.fromRGB(math.random(1, 225), math.random(1, 225), math.random(1, 225))
			Jihad.wait(1)
		end
	end)

	TabContainer.Name = "TabContainer"
	TabContainer.Parent = SideFrame
	TabContainer.Active = true
	TabContainer.BackgroundColor3 = theme.secondary
	TabContainer.BackgroundTransparency = 1.000
	TabContainer.BorderSizePixel = 0
	TabContainer.Position = UDim2.new(0.0510812625, 0, 0.0220125783, 0)
	TabContainer.Size = UDim2.new(0, 117, 0, 305)
	TabContainer.CanvasSize = UDim2.new(0, 0, 0, 0)
	TabContainer.ScrollBarThickness = 0

	TabContainerL.Name = "TabContainerL"
	TabContainerL.Parent = TabContainer
	TabContainerL.HorizontalAlignment = Enum.HorizontalAlignment.Center
	TabContainerL.SortOrder = Enum.SortOrder.LayoutOrder
	TabContainerL.Padding = UDim.new(0, 5)

	TabContainerP.Name = "TabContainerP"
	TabContainerP.Parent = TabContainer
	TabContainerP.PaddingTop = UDim.new(0, 5)

	TabHolder.Name = "TabHolder"
	TabHolder.Parent = MainFrame
	TabHolder.BorderSizePixel = 0
	TabHolder.BackgroundColor3 = theme.secondary
	TabHolder.Position = UDim2.new(0.244000047, 0, 0.117999971, 0)
	TabHolder.Size = UDim2.new(0, 440, 0, 318)
	TabHolder.ClipsDescendants = true

	TabHolderC.CornerRadius = UDim.new(0, 4)
	TabHolderC.Name = "TabHolderC"
	TabHolderC.Parent = TabHolder

	TopFrame.Name = "TopFrame"
	TopFrame.Parent = MainFrame
	TopFrame.BackgroundColor3 = theme.secondary
	TopFrame.Position = UDim2.new(0.0123288939, 0, 0.0191256832, 0)
	TopFrame.Size = UDim2.new(0, 575, 0, 33)

	SideFrameC_2.CornerRadius = UDim.new(0, 4)
	SideFrameC_2.Name = "SideFrameC"
	SideFrameC_2.Parent = TopFrame

	Title.Name = "Title"
	Title.Parent = TopFrame
	Title.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	Title.BackgroundTransparency = 1.000
	Title.BorderSizePixel = 0
	Title.Position = UDim2.new(0.0112130605, 0, 0, 0)
	Title.Size = UDim2.new(0, 569, 0, 33)
	Title.Font = Enum.Font.GothamMedium
	Title.Text = title
	Title.TextColor3 = theme.accent
	Title.TextSize = 16.000
	Title.TextXAlignment = Enum.TextXAlignment.Left

	drag(MainFrame, TopFrame)

	TabContainerL:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(function()
		TabContainer.CanvasSize = UDim2.new(0, 0, 0, TabContainerL.AbsoluteContentSize.Y + 18)
	end)

	local Holder = {}

	function Holder:Tab(name,icon)
		local TabOpen = Instance.new("TextButton")
		local TabOpenC = Instance.new("UICorner")
		local TabIcon = Instance.new("ImageLabel")
		local TabOpenP = Instance.new("UIPadding")
		local Section = Instance.new("ScrollingFrame")
		local SectionP = Instance.new("UIPadding")
		local TabHolderC = Instance.new("UICorner")
		local SectionL = Instance.new("UIListLayout")

		TabOpen.Name = "TabOpen"
		TabOpen.Parent = TabContainer
		TabOpen.BackgroundColor3 = Color3.fromRGB(42, 42, 42)
		TabOpen.BackgroundTransparency = 1.000
		TabOpen.BorderSizePixel = 0
		TabOpen.Position = UDim2.new(0, 0, 0.0216489695, 0)
		TabOpen.Size = UDim2.new(0, 116, 0, 30)
		TabOpen.AutoButtonColor = false
		TabOpen.Font = Enum.Font.GothamMedium
		TabOpen.Text = ("       %s"):format(name)
		TabOpen.TextColor3 = theme.accent
		TabOpen.TextSize = 14.000
		TabOpen.TextXAlignment = Enum.TextXAlignment.Left

		TabOpenC.CornerRadius = UDim.new(0, 4)
		TabOpenC.Name = "TabOpenC"
		TabOpenC.Parent = TabOpen

		TabIcon.Name = "TabIcon"
		TabIcon.Parent = TabOpen
		TabIcon.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
		TabIcon.BackgroundTransparency = 1.000
		TabIcon.Position = UDim2.new(0, 0, 0.166666672, 0)
		TabIcon.Size = UDim2.new(0, 20, 0, 20)
		TabIcon.Image = ("rbxassetid://%s"):format((icon or 4370341699))
		TabIcon.ImageColor3 = theme.accent
		TabIcon.ScaleType = Enum.ScaleType.Fit

		TabOpenP.Name = "TabOpenP"
		TabOpenP.Parent = TabOpen
		TabOpenP.PaddingLeft = UDim.new(0, 5)

		Section.Name = name
		Section.Parent = TabHolder
		Section.Active = true
		Section.BackgroundColor3 = theme.secondary
		Section.BorderSizePixel = 0
		Section.Position = UDim2.new(-0.00145454495, 0, -0.00149685144, 0)
